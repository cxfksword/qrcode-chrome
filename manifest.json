{
	"name": "qrcode-chrome",
	"version": "0.1.2",
	"manifest_version": 2,
	"homepage_url" : "https://github.com/cxfksword/qrcode-chrome" ,
	"description": "QR code generated extension for chrome",
	"browser_action": {
		"default_icon": {
	         "19": "img/icon19.png",
	         "38": "img/icon38.png"
	      },
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs"
	],
	"icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   }
}
